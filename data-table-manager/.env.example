# Environment variables for Data Table Manager

# Entra ID (Azure AD) Configuration
REACT_APP_ENTRA_CLIENT_ID=your-entra-client-id
REACT_APP_ENTRA_AUTHORITY=https://login.microsoftonline.com/your-tenant-id
REACT_APP_REDIRECT_URI=http://localhost:3000

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_USER_POOL_ID=your-user-pool-id
REACT_APP_USER_POOL_CLIENT_ID=your-user-pool-client-id
REACT_APP_IDENTITY_POOL_ID=your-identity-pool-id
REACT_APP_OAUTH_DOMAIN=your-oauth-domain.auth.us-east-1.amazoncognito.com

# Databricks Configuration
REACT_APP_DATABRICKS_HOST=your-databricks-host
REACT_APP_DATABRICKS_TOKEN=your-databricks-token
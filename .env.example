# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_USER_POOL_ID=your_user_pool_id
REACT_APP_USER_POOL_CLIENT_ID=your_user_pool_client_id
REACT_APP_IDENTITY_POOL_ID=your_identity_pool_id
REACT_APP_OAUTH_DOMAIN=your_oauth_domain

# Entra ID Configuration
REACT_APP_ENTRA_CLIENT_ID=your_entra_client_id
REACT_APP_ENTRA_AUTHORITY=https://login.microsoftonline.com/your_tenant_id
REACT_APP_REDIRECT_URI=http://localhost:3000

# Databricks Configuration
REACT_APP_DATABRICKS_HOSTNAME=your_databricks_hostname
REACT_APP_DATABRICKS_HTTP_PATH=your_databricks_http_path
REACT_APP_DATABRICKS_TOKEN=your_databricks_token
REACT_APP_DATABRICKS_CATALOG=main
REACT_APP_DATABRICKS_SCHEMA=default

# Application Configuration
REACT_APP_API_BASE_URL=http://localhost:3000/api
REACT_APP_ENVIRONMENT=development
REACT_APP_LOG_LEVEL=debug

# Feature Flags
REACT_APP_ENABLE_CSV_UPLOAD=true
REACT_APP_ENABLE_BULK_OPERATIONS=true
REACT_APP_MAX_UPLOAD_SIZE=10485760
REACT_APP_PAGE_SIZE=50